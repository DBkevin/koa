<li class="media mt-4 mb-4">
  <a href="/users/<%= user.id %>">
    <img src="<%= user.avatar %>" alt="<%=user.avatar %>" class="mr-3 gravatar"/>
  </a>
  <div class="media-body">
    <h5 class="mt-0 mb-1"><%=user.name %> <small> / <%=post.createdAt %></small></h5>
    <%= post.id %>|
    <%= post.centent %>
  </div>
</li>